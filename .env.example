# KiotViet API Configuration
KIOTVIET_USERNAME=your_username
KIOTVIET_PASSWORD=your_password
KIOTVIET_RETAILER_ID=your_retailer_id
KIOTVIET_BRANCH_ID=your_branch_id

# API Settings
API_BASE_URL=https://api-man1.kiotviet.vn/api
API_TIMEOUT=30
API_MAX_RETRIES=3
API_PAGE_SIZE=100

# Data Paths
DATA_OUTPUT_PATH=data/output
DATA_CHECKPOINT_PATH=data/checkpoints
LOG_PATH=data/logs

# Azure Blob Storage (optional)
AZURE_STORAGE_CONNECTION_STRING=your_azure_connection_string
AZURE_STORAGE_CONTAINER=kiotviet-data

# Environment
ENV=development
